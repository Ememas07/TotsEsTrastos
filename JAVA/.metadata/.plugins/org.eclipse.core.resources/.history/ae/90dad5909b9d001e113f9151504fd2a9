	if(-4*A*C <0 || 2*A == 0) {
		System.out.println("No té solució");
	}
	
	
	
	
	
	//Parts de l'equació: -b, -4ac, sqrt, 2a, 
	
	
	

	
	

	
	
		
		
		
	
	
	
	
	
	
